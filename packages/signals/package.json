    "url": "https://github.com/ibnlanre/ibnlanre.git",
    "type": "git",
    "directory": "packages/signals",
    "publishConfig": {
      "access": "public"
    }
  },
  "bugs": {
    "url": "https://github.com/ibnlanre/portal/issues"
  },
  "license": "MIT",
  "keywords": [
    "signal",
    "solid-js",
    "solid",
    "reactive",
