{
  "name": "@ibnlanre/atom",
  "version": "0.0.1",
  "description": "Charts components built with recharts and Mantine",
  "homepage": "https://github.com/ibnlanre/portal#readme",
  "license": "MIT",
  "author": "Ridwan Olanrewaju <olaitanlanre28@gmail.com>",
  "keywords": [
    "react",
    "ui"
  ],
  "scripts": {
    "lint": "tsc --pretty --noEmit --project tsconfig.json"
  },
  "main": "./cjs/index.cjs",
  "module": "./esm/index.mjs",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./lib/index.d.mts",
        "default": "./esm/index.mjs"
      },
      "require": {
        "types": "./lib/index.d.ts",
        "default": "./cjs/index.cjs"
      }
    },
    "./styles.css": "./styles.css",
    "./styles.layer.css": "./styles.layer.css"
  },
  "repository": {
    "url": "https://github.com/ibnlanre/ibnlanre.git",
    "type": "git",
    "directory": "packages/atom"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
